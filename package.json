{
  "name": "@moonjellydigital/prettier-config",
  "description": "Prettier shareable config",
  "version": "1.0.0",
  "main": "index.json",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "author": "Camille Spain <camille@camillespain.com>",
  "license": "MIT",
  "keywords": [
    "prettier",
    "prettier-config"
  ],
  "homepage": "https://github.com/moonjellydigital/prettier-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moonjellydigital/prettier-config.git"
  },
  "files": [
    "./index.json"
  ],
  "scripts": {
    "test": "bun test",
    "prepare": "husky || true",
    "tsc:ci": "bunx tsc",
    "fmt:ci": "bunx prettier --check .",
    "lint:ci": "bunx eslint ."
  },
  "lint-staged": {
    "*.{json,yml,yaml,md}": "prettier --write",
    "*.{ts,js}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/bun": "^1.1.11",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/prettier": "^3.0.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1"
  }
}
